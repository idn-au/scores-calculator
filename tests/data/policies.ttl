PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX odrl-ext: <https://example.com/ext/>
PREFIX in: <http://data.idnau.org/pid/vocab/lc-in/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>


in:open-to-collaborate
    a odrl:Offer , skos:Concept ;
    odrl:action odrl-ext:collaborate ;
    skos:prefLabel "Open to Collaborate"@en ;
.

odrl-ext:collaborate a odrl:Action ;
    rdfs:label "Collaborate"@en ;
    rdfs:comment "The ability to collaboratively view, comment, edit, and share the asset."@en ;
.